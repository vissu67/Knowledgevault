<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.dao.bean.Config" table="OKM_CONFIG">
    <id name="key" column="CFG_KEY"><generator class="assigned"/></id>
    <property name="type" column="CFG_TYPE" not-null="true"/>
    <property name="value" column="CFG_VALUE" type="text"/>
  </class>
</hibernate-mapping>
