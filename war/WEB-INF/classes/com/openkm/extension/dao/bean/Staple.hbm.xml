<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.openkm.extension.dao.bean.Staple" table="OKM_STAPLE">
    <id name="id" column="ST_ID"><generator class="native"/></id>
    <property name="uuid" column="ST_UUID" not-null="true"/>
    <property name="type" column="ST_TYPE" not-null="true"/>
  </class>
</hibernate-mapping>
